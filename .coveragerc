[run]
omit =
    */test/*
    */venv*
branch = True
dynamic_context = test_function

[paths]
source = shelllogger

[report]
skip_covered = False
fail_under = 80
show_missing = True
exclude_lines =
    pragma: no cover
    if 0:
    if False:

[html]
directory = test/htmlcov
title = shell-logger Coverage Report
show_contexts = True

[xml]
output = test/coverage.xml

[json]
output = test/coverage.json
pretty_print = True
show_contexts = True
